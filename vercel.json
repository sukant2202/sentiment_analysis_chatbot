{
  "version": 2,
  "builds": [
    {
      "src": "index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "index.py"
    }
  ],
  "env": {
    "NLTK_DATA_PATH": "/tmp/nltk_data"
  },
  "functions": {
    "index.py": {
      "maxDuration": 10
    }
  }
}
